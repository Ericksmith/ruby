<h1>Listing <%= @dojos.length %> Dojos</h1>

<table>
  <tr>
    <th>Branch</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Actions</th>
  </tr>
  <% if @dojos %>
    <% @dojos.each do |d|%>
    <tr>
      <td><%= d.branch %></td>
      <td><%= d.street %></td>
      <td><%= d.city %></td>
      <td><%= d.state %></td>
      <td><a href="/dojos/<%= d.id %>">Show</a> | <a href="/dojos/<%= d.id %>/edit">Edit</a> | <a href="/dojos/<%= d.id %>">Destroy</a></td>
    </tr>
    <% end %>
  <% end %>
</table>

<a href="/dojos/new">New Dojo</a>