<h1>New Dojo</h1>

<% if flash[:message] %>
  <% flash[:message].each do | m |%>
  <p><%= m %></p>
  <%end%>
<%end%>
<form action="/create" method="post">
<input type='hidden' name='authenticity_token' value='<%= form_authenticity_token =%>'>
<p>Branch: <input type="text" name="dojo[branch]"></p>
<p>Street: <input type="text" name="dojo[street]"></p>
<p>City: <input type="text" name="dojo[city]"></p>
<p>State: <input type="text" name="dojo[state]"></p>
<input type="submit" value="Create">
</form>

<a href="/dojos">Go back</a>